{
	"compilerOptions": {
		"target": "esnext",
		"lib": [
			"dom",
			"dom.iterable",
			"esnext"
		],
		"allowJs": true,
		"skipLibCheck": true,
		"strict": true,
		"forceConsistentCasingInFileNames": true,
		"noEmit": false,
		"outDir": "./dist",
		"declaration": true,
		"esModuleInterop": true,
		"module": "esnext",
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"isolatedModules": true,
		"allowSyntheticDefaultImports": true,
		"jsx": "react-jsx",
		"jsxImportSource": "theme-ui",
		"jsxFactory": "",
		"jsxFragmentFactory": "",
		"incremental": true
	},
	"include": [
		"src"
	]
}